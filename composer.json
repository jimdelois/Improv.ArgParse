  "type": "library",
  "keywords": [
    "improv",
    "improvframework",
    "cli",
    "argparse",
    "argument parsing"
  ],
  "homepage": "https://github.com/improvframework/Improv.ArgParse/",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Jim DeLois",
      "email": "jim.delois@improvframework.org",
      "homepage": "http://www.improvframework.org/"
    }
  ],
  "prefer-dist": true,
  "config": {
    "use-include-path": true
  },
  "require": {
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "phake/phake" : "~2.0",
    "fiunchinho/phpunit-randomizer": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Improv\\ArgParse\\": "src/"
